{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "react-app/jest"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "warn",
    "quotes": ["error", "single"],
    "no-plusplus": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-filename-extension": "off"
  }
}
